Cell[BoxData[
 RowBox[{
  RowBox[{"randomCover", "[", 
   RowBox[{"radius_", ",", "failedLimit_"}], "]"}], ":=", " ", 
  RowBox[{"Module", "[", "\n", "\t\t ", 
   RowBox[{
    RowBox[{"{", "}"}], ",", "\n", "\t\t ", 
    RowBox[{
     RowBox[{"regionCovered", " ", "=", " ", 
      RowBox[{"Ball", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"0", ",", "0"}], "}"}], ",", 
        RowBox[{"1", "/", "300"}]}], "]"}]}], ";", "\n", "         ", 
     RowBox[{"failedCount", " ", "=", " ", "0"}], ";", "\[IndentingNewLine]", 
     "\t\t ", 
     RowBox[{"circleCount", " ", "=", " ", "0"}], ";", "\n", "\t\t ", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"failedCount", " ", "<", " ", "failedLimit"}], ",", " ", 
       "\[IndentingNewLine]", "\t\t\t", 
       RowBox[{"Module", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"randomCenter", " ", "=", " ", 
           RowBox[{"determineCenter", "[", 
            RowBox[{"randomIndex", ",", " ", "randomLocation"}], "]"}]}], 
          "}"}], ",", " ", "\n", "\t\t\t\t", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"RegionMember", "[", 
            RowBox[{"regionCovered", ",", " ", "randomCenter"}], "]"}], ",", 
           " ", "\n", "\t\t\t\t\t", 
           RowBox[{"failedCount", "++"}], ",", " ", "\n", "\t\t\t\t\t", 
           RowBox[{
            RowBox[{"failedCount", " ", "=", " ", "0"}], ";", " ", 
            RowBox[{"circleCount", "++"}], ";", " ", 
            RowBox[{"regionCovered", " ", "=", " ", 
             RowBox[{"RegionUnion", "[", 
              RowBox[{"regionCovered", ",", " ", 
               RowBox[{"Ball", "[", 
                RowBox[{"randomCenter", ",", " ", "radius"}], "]"}]}], 
              "]"}]}]}]}], "]"}]}], "]"}]}], "]"}], ";", " ", "\n", 
     "         ", "circleCount"}]}], "\n", "         ", "]"}]}]], "Code",
