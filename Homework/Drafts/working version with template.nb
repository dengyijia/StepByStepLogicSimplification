       GridBoxAlignment -> {"Columns" -> {{Left}}}, DefaultBaseStyle -> 
       "Column", 
       GridBoxItemSize -> {
        "Columns" -> {{Automatic}}, "Rows" -> {{Automatic}}}],Dynamic[
      Typeset`open]},
     "IconizedObject"]],
   Show[
    Graphics[{Blue, 
      $CellContext`coverOfCurve[4]}], 
    Graphics[{Thick, 
      KochCurve[4]}]],
   SelectWithContents->True,
   Selectable->False]}]], "Input",
